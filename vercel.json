{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "/index.js",
      "methods": ["GET"]
    },
    {
      "src": "/user/test",
      "dest": "/index.js",
      "methods": ["GET"]
    },
    {
      "src": "/user/login",
      "dest": "/index.js",
      "methods": ["POST"]
    },
    {
      "src": "/user/signup",
      "dest": "/index.js",
      "methods": ["POST"]
    },
    {
      "src": "/user/secure",
      "dest": "/index.js",
      "methods": ["GET"]
    },
    {
      "src": "/posts/all",
      "dest": "/index.js",
      "methods": ["GET"]
    }
  ]
}
